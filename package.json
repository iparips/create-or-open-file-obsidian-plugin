{
	"name": "regex-file-open-or-create-command",
	"version": "1.0.0",
	"description": "This is an Obsidian plugin that allows creation of commands that open files matching a regex.",
	"main": "main.js",
	"type": "commonjs",
	"scripts": {
		"build": "bun build src/main.ts --outdir . --external obsidian --format cjs",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "bunx eslint .",
		"lint:fix": "bunx eslint . --fix",
		"format": "bunx prettier --write ."
	},
	"keywords": [],
	"author": "Ilya Paripsa",
	"license": "GPL",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"prettier": "^3.5.3",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"date-fns": "^4.1.0",
		"obsidian": "latest"
	}
}
