{
	"name": "create-or-open-file",
	"version": "1.1.9",
	"description": "This is an Obsidian plugin that allows creation of commands that open files matching a regex.",
	"main": "main.js",
	"scripts": {
		"build": "bun run test && bun build src/main.ts --outdir . --entry-naming \"[name].[ext]\" --external obsidian --format cjs --sourcemap",
		"version:major": "bun run version-bump.mjs major",
		"version:minor": "bun run version-bump.mjs minor",
		"version:patch": "bun run version-bump.mjs patch",
		"lint": "bunx eslint .",
		"lint:fix": "bunx eslint . --fix",
		"format": "bunx prettier --write .",
		"test": "vitest run"
	},
	"keywords": [],
	"author": "Ilya Paripsa",
	"license": "GPL",
	"devDependencies": {
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^16.11.6",
		"@types/react": "^19.1.6",
		"@types/react-dom": "^19.1.5",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"@vitest/coverage-v8": "^1.3.1",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"jsdom": "^26.1.0",
		"prettier": "^3.5.3",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"vitest": "^1.3.1"
	},
	"dependencies": {
		"date-fns": "^4.1.0",
		"obsidian": "latest",
		"react": "^19.1.0",
		"react-dom": "^19.1.0"
	}
}
